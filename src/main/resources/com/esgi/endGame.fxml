<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.esgi.controllers.EndGameController">
    <children>
        <ImageView fitHeight="500.0" fitWidth="800.0" opacity="0.75">
                <image>
                    <Image url="@image/background.jpg" />
            </image>
        </ImageView>
      <Text fx:id="textMessageEndGame" layoutY="154.0" strokeType="OUTSIDE" strokeWidth="0.0" text="End Game message" textAlignment="CENTER" wrappingWidth="800.0">
         <font>
            <Font size="50.0" />
         </font>
      </Text>
      <Text fx:id="textDictatorName" layoutY="200.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Dictator Name" textAlignment="CENTER" wrappingWidth="800.0">
         <font>
            <Font size="15.0" />
         </font>
      </Text>
      <Text fx:id="textTime" layoutY="260.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Time" textAlignment="CENTER" wrappingWidth="800.0">
         <font>
            <Font size="15.0" />
         </font>
      </Text>
      <Text fx:id="textNameCampaign" layoutY="230.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name Campaign" textAlignment="CENTER" wrappingWidth="800.0">
         <font>
            <Font size="15.0" />
         </font>
      </Text>
      <Button fx:id="buttonMenu" layoutX="339.0" layoutY="300.0" mnemonicParsing="false" onAction="#switchToMenu" prefHeight="25.0" prefWidth="123.0" text="Quit" />
    </children>

</AnchorPane>
