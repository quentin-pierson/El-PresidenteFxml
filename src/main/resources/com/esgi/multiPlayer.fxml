<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.esgi.MultiplayerController">
   <children>
      <ImageView fitHeight="401.0" fitWidth="602.0" opacity="0.75">
         <image>
            <Image url="@image/background.jpg" />
