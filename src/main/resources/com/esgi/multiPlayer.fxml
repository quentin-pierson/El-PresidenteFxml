      </Button>
      <Button fx:id="buttonJoinGame" layoutX="246.0" layoutY="201.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="106.0" text="Join Game">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="buttonBack" layoutX="246.0" layoutY="240.0" mnemonicParsing="false" onAction="#switchToPlay" prefHeight="31.0" prefWidth="106.0" text="Back">
